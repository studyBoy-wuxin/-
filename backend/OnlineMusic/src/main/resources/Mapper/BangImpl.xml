<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.gdut.onlinemusic.Dao.BangDao">
    <select id="findAllBang" resultType="Bang">
        select * from bang
    </select>

    <select id="findBangByID" resultType="Bang">
        select * from bang where id=#{id}
    </select>
</mapper>